{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{CatchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "servicepost",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/ServicePost/{CatchAll}"
    },
    {
      "DownstreamPathTemplate": "/{CatchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "imageapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/ImageApi/{CatchAll}"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}