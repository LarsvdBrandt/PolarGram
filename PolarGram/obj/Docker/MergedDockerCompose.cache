services:
  apigateway:
    build:
      context: D:\repos\PolarGram\PolarGram
      dockerfile: APIGateway/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: apigateway
    ports:
    - published: 5000
      target: 80
    - published: 6000
      target: 443
    volumes:
    - C:\Users\larsv\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\larsv\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  frontend:
    build:
      context: D:\repos\PolarGram\PolarGram
      dockerfile: ./frontend/Dockerfile
    container_name: frontend
    environment:
      CHOKIDAR_USEPOLLING: "true"
    image: frontend
    ports:
    - published: 3001
      target: 3000
    stdin_open: true
    volumes:
    - D:\repos\PolarGram\PolarGram\frontend:/app:rw
  imageapi:
    build:
      context: D:\repos\PolarGram\PolarGram
      dockerfile: ImageApi/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: imageapi
    ports:
    - target: 80
  rabbit:
    hostname: rabbit
    image: rabbitmq:3.7.2-management
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
  servicecomment:
    build:
      context: D:\repos\PolarGram\PolarGram
      dockerfile: ServiceComments/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: servicecomment
    ports:
    - target: 80
  servicepost:
    build:
      context: D:\repos\PolarGram\PolarGram
      dockerfile: ServicePost/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: servicepost
    ports:
    - target: 80
version: '3.4'